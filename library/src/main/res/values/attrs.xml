<?xml version="1.0" encoding="utf-8"?>

<resources>
    <!--PowerView-->
    <declare-styleable name="PowerView">
        <attr name="containerType" format="enum">
            <enum name="LinearLayout" value="0" />
            <enum name="RelativeLayout" value="1" />
        </attr>
        <attr name="viewPosition" format="integer" />
        <attr name="withSwipeRefreshLayout" format="boolean" />
        <attr name="withProgressBar" format="boolean" />
        <attr name="withRecyclerView" format="boolean" />
        <attr name="recyclerViewLayoutAnimation" format="reference" />
        <attr name="disableLayoutAnimation" format="boolean" />
        <attr name="emptyStateLayoutId" format="reference" />
    </declare-styleable>
</resources>